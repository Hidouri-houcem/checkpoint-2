ALGORITHM InsertionSort
VAR
    arr : ARRAY_OF INTEGER;
    n: INTEGER;
    i,j,key:INTEGER;
BEGIN
    n := Lenghth(arr);


    FOR i FROM 1 TO (n-1) STEP 1  DO
        key := arr[i];
        j := i-1;



        WHILE (j â‰¥ 0 and arr[j] > key) DO
            arr[j + 1] := arr[j];
            j := j - 1;  
        END_WHILE



        arr[j + 1] := key;
    END_FOR


    retourner arr;
END